<%= render 'books/list', books: @books, user: @user %>

<div class="col-md-8 offset-md-1">
  <h2>Users</h2>
    <table class="table">
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @books.each do |book| %>
          <tr>
            <td><%= image_tag @user.get_profile_image(100,100) %></td>
            <td><%= @user.name %></td>
            <td><%= link_to "Show", users_path %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
</div>